{ 
  "manifest_version": 3,
  "name": "X.com Image Blackout",
  "version": "1.0",
  "description": "Black out images and videos on X.com timeline with click to reveal",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://x.com/*", "*://twitter.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}
